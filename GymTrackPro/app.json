{
  "expo": {
    "name": "GymTrackPro",
    "slug": "GymTrackPro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gymtrackpro",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "backgroundColor": "#1C1C1E"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends and set profile pictures.",
          "cameraPermission": "The app accesses your camera to let you take profile pictures."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow GymTrackPro to access your camera."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow GymTrackPro to access your photos.",
          "savePhotosPermission": "Allow GymTrackPro to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-sqlite",
      "expo-font",
      "expo-router",
      [
        "sentry-expo",
        {
          "organization": "yourorganization",
          "project": "gymtrackpro",
          "url": "https://sentry.io/",
          "hooks": {
            "postPublish": {
              "sourceMaps": true,
              "includes": ["dist"]
            }
          }
        }
      ]
    ],
    "jsEngine": "hermes",
    "description": "A comprehensive fitness tracking application for tracking workouts, weight, and connecting with friends.",
    "newArchEnabled": true,
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "yourorganization",
            "project": "gymtrackpro",
            "authToken": "YOUR_SENTRY_AUTH_TOKEN"
          }
        }
      ]
    }
  }
}
